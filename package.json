{
  "name": "hope-studio",
  "version": "2.0.0",
  "description": "Official website for Hope Studio",
  "scripts": {
    "build": "vuepress build src",
    "dev": "vuepress dev src",
    "dev:clean": "vuepress dev src --clean-cache",
    "lint": "prettier --check --write . && markdownlint **/*.md --ignore node_modules --fix",
    "lint:check": "prettier --check . && markdownlint **/*.md --ignore node_modules",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "author": "Mr.Hope",
  "license": "MIT",
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.53",
    "vue": "3.2.45",
    "vuepress": "2.0.0-beta.53",
    "vuepress-theme-hope": "2.0.0-beta.122"
  },
  "devDependencies": {
    "markdownlint-cli": "0.32.2",
    "prettier": "2.7.1"
  },
  "packageManager": "pnpm@7.17.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@types/react",
        "react",
        "react-dom",
        "webpack"
      ]
    }
  }
}
